    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "sass": "^1.94.2",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
